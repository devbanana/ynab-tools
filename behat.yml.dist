default:
    suites:
        default:
            contexts:
                - App\Tests\Behat\SecurityContext

    extensions:
        FriendsOfBehat\SymfonyExtension:
            bootstrap: 'tests/bootstrap.php'
            kernel:
                environment: test
                debug: true
        Behat\MinkExtension:
            sessions:
                symfony:
                    symfony: ~
